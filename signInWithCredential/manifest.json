{
  "manifest_version": 3,
  "name": "Firebase Auth - signInWithCredential",
  "description": "This sample shows how to authorize Firebase via signInWithCredential in a Chrome extension with manifest v3",
  "version": "0.1",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "offscreen",
    "identity"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "web_accessible_resources": [
    {
      "resources": [ "signInWithCredential_background.js" ],
      "matches": [ "https://positive-fanatical-machine.glitch.me/*" ]
    }
  ],
  "key":""
}
